<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppEditText">
        <attr name="aet_hint" format="string"/>
        <attr name="aet_text" format="string"/>
        <attr name="aet_inputType" format="enum">
            <enum name="password" value="0"/>
            <enum name="phone" value="1"/>
            <enum name="email" value="2"/>
            <enum name="text" value="3"/>
            <enum name="number" value="10"/>
            <enum name="textMultiline" value="4"/>
        </attr>
        <attr name="aet_postfix" format="string"/>
        <attr name="aet_imeOption" format="enum">
            <enum name="actionDone" value="5"/>
            <enum name="actionNext" value="6"/>
            <enum name="actionOk" value="7"/>
        </attr>
    </declare-styleable>

</resources>